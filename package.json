{
  "name": "nimble-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "cargo build",
    "build:cli": "cargo build --release --features cli --bin nimble",
    "test": "cargo test",
    "test:coverage": "cargo llvm-cov --all-features --summary-only",
    "start:light": "cargo run --example light_server",
    "start:light:env": "NIMBLE_ADDRESS=127.0.0.1:8080 cargo run --example light_server",
    "start:photos": "cargo run --example photos"
  }
}